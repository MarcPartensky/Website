$(".nav-list a").on("click", function (e) {
    // 1
    e.preventDefault();
    // 2
    const href = $(this).attr("href")
    console.log(href)
    const element = document.getElementById(href)
    console.log(element)
    // console.log(href)
    // console.log('href should be logged')
    // console.log($(href))

    // console.log(href)
    // 3
    $("html, body").animate({ scrollTop: $(element).offset().top }, 800);
});

